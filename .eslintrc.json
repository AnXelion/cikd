{
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "meteor": true
  },
    "extends": [
      "airbnb",
      "plugin:react/recommended",
      "plugin:meteor/recommended"
    ],
    "plugins": [
        "react",
        "meteor"
    ]
}
